{% extends 'base.html.twig' %}

{% block title %}Custom{% endblock %}

{% block body %}
    <h1>Custom</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ custom.id }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ custom.price }}</td>
            </tr>
            <tr>
                <th>Payment_date</th>
                <td>{{ custom.paymentDate ? custom.paymentDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Complete_date</th>
                <td>{{ custom.completeDate ? custom.completeDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ custom.address }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_custom_index') }}">back to list</a>

    <a href="{{ path('app_custom_edit', {'id': custom.id}) }}">edit</a>

    {{ include('custom/_delete_form.html.twig') }}
{% endblock %}
