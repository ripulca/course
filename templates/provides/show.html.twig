{% extends 'base.html.twig' %}

{% block title %}Provides{% endblock %}

{% block body %}
    <h1>Provides</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ provide.id }}</td>
            </tr>
            <tr>
                <th>Provider</th>
                <td>{{ provide.provider }}</td>
            </tr>
            <tr>
                <th>Medicine</th>
                <td>{{ provide.medicine }}</td>
            </tr>
            <tr>
                <th>Amount</th>
                <td>{{ provide.amount }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_provides_index') }}">back to list</a>

    <a href="{{ path('app_provides_edit', {'id': provide.id}) }}">edit</a>

    {{ include('provides/_delete_form.html.twig') }}
{% endblock %}
